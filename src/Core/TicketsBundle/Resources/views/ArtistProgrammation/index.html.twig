{% extends "::layout.html.twig" %}

{% block body %}

<h1>ArtistProgrammation list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Artist_id</th>
            <th>Programmation_id</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('artistprogrammation_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.artistid }}</td>
            <td>{{ entity.programmationid }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('artistprogrammation_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('artistprogrammation_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('artistprogrammation_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
{% endblock %}