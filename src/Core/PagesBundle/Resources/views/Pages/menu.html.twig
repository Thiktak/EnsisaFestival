
              <h1>Et aussi ...</h1>
              <h2>Places restantes</h2>
              <ul>
                {% for ticket in tickets %}
                {% if ticket.programmations|length >= 1 %}
                <li>{{ ticket.title }} : {{ticket.stock}}</li>
                {% endif %}

                {#
                <li class="divider"></li>

                {% if ticket.programmations|length > 1 %}
                <li>{{ ticket.title }}</li>
                {% endif %}#}

                {% endfor %}
              </ul>
              
              <h2>La selection du jour</h2>
              <figure class="user">
                <img src="{{ asset('img/artistes/' ~ artist.image )}}" alt="{{ artist }}" />
                <figcaption>
                  <span class="name">{{ artist }}</span>
                  <span class="info">{{ artist.gender }} - {{ artist.country }}</span>
                </figcaption>
              </figure>
              
              <h2>Et ailleurs</h2>
              <ul>
                <li><a href="?page=accueil">Festival les inRocks</a></li>
              </ul>

